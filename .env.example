# Copy this file to .env and fill in values

# LDAP connection
LDAP_SERVER=ldap.example.com
LDAP_PORT=389
# Suffix appended to login for bind DN. For AD typically your domain, e.g., example.com
LDAP_BIND_DN=example.com
# Base DN to search users, e.g., DC=example,DC=com
LDAP_BASE_DN=DC=example,DC=com

# App
PORT=8000
RELOAD=0
